{
  "name": "bumpkey",
  "version": "0.0.1",
  "description": "An offensive tool for tampering lock files",
  "main": "src/index.js",
  "bin": {
    "bumpkey": "bin/index.js"
  },
  "scripts": {
    "build": "npm run clean && npm run test && ./node_modules/.bin/babel ./src --out-dir ./bin",
    "clean": "rm -rf ./bin ./test-data/node_modules",
    "configure-tests": "npm i --prefix ./test-data/ ./test-data/",
    "test": "npm run configure-tests && jest --config jest.config.js",
    "install-global": "npm run clean && npm run build && npm i -g ."
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "lock",
    "file",
    "tamper",
    "tampering"
  ],
  "author": "gitlab.com/codeEmitter",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.13.4",
    "@babel/preset-env": "^7.12.11",
    "bumpkey-test-data": "file:test-data",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "npm-check": "^5.9.2",
    "read-package-json": "^3.0.1",
    "yargs": "^16.2.0"
  }
}
