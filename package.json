{
  "name": "bump-key",
  "description": "An offensive tool for tampering lock files",
  "main": "src/index.js",
  "type": "module",
  "engine": ">=15.0.0",
  "bin": {
    "bump-key": "src/index.js"
  },
  "scripts": {
    "build": "npm install && npm run lint && npm run test",
    "clean": "rm -rf ./node_modules",
    "install-global": "npm run clean && npm run build && npm i -g .",
    "lint": "npx eslint ./src/**/*.js",
    "test": "npx mocha --recursive ./src/**/*spec.js"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/gitlab"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/gitlab-com/gl-security/security-operations/gl-redteam/bump-key.git"
  },
  "keywords": [
    "lock file tampering",
    "lock file",
    "lockfile",
    "tamper",
    "tampering"
  ],
  "author": "gitlab.com/codeEmitter",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "chai": "^4.3.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.1",
    "sinon": "^9.2.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "npm": "^7.6.3",
    "npm-check": "^5.9.2",
    "yargs": "^16.2.0"
  }
}
